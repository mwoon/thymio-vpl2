            {
         	"cmd": "multi",
                "scene":
                   [
                      { 
                         "cmd": "text", 
                         "text": "story2.55"
                      },
		      {
                         "cmd": "dialogue",
                         "speaker": "Ada",
                         "text": "Oh, kind of like not being able to see in the dark, right?" 
